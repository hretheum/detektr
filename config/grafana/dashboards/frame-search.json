          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "histogram_quantile(0.95, frame_processing_duration_seconds_bucket{frame_id=\"$frame_id\"}) * 1000",
          "legendFormat": "{{stage}}",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Frame Processing Duration by Stage",
      "type": "timeseries"
    },
